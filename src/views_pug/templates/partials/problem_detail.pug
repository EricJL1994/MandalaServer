-
  const lookupColorName = {
    Pink: "Rosa",
    Green: "Verde",
    Orange: "Naranja",
    Yellow: "Amarillo",
    Red: "Rojo",
  }
-    
  var card = {
    src: problemDetail.image,
    alt: `${lookupColorName[problemDetail.difficultyName]} ${problemDetail.number}`,
    tittle: `${lookupColorName[problemDetail.difficultyName]} ${problemDetail.number}`,
    description: `<strong>Color de presas:</strong> ${problemDetail.holdColor} <br> <strong>Pared:</strong> ${problemDetail.wallName} <br> <strong>Fecha:</strong> ${problemDetail.date} <br> <strong>Encadenados:</strong> ${problemDetail.redpoints.length}`,
    link: undefined,
    list: problemDetail.list,
    userList: problemDetail.userList
  }
include card.pug
if(user && user.admin)
  a(href=`/admin/editBoulder/${problemDetail._id}`) Editar